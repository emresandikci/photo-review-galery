{
  "name": "photo-review-galery",
  "version": "1.0.0",
  "description": "A React.js easy starter boilerplate",
  "main": "index.js",
  "repository": "https://github.com/emresandikci/photo-review-galery",
  "author": "emresandikci<me@emresandikci.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/emresandikci/photo-review-galery/issues"
  },
  "homepage": "https://github.com/emresandikci/photo-review-galery#readme",
  "scripts": {
    "dev": "webpack serve --open --mode development",
    "dev:beta": "webpack --open --mode none",
    "dev:mobile": "webpack --open --mode development --mobile",
    "start": "webpack --open --mode production",
    "build": "yarn clean && yarn lint:fix && yarn format && webpack --mode production",
    "build:dev": "yarn clean && yarn lint:fix && yarn format && webpack --mode development",
    "build:icons": "yarn svgr ./src/public/icons -d ./src/icons --svgo-config .svgorc.json",
    "clean": "rimraf dist/",
    "format": "prettier --write \"src/**/*.{js,jsx,css,json}\"",
    "lint:watch": "esw src -w --color",
    "lint": "eslint src",
    "lint:fix": "yarn lint --fix",
    "validate": "prettier --check \"src/**/*.{js,jsx,css,json}\" && yarn lint",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/!(*story).js": [
      "yarn format",
      "yarn lint:fix",
      "yarn validate"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.13.7",
    "@reduxjs/toolkit": "^1.6.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-hot-loader": "^4.13.0",
    "react-redux": "^7.2.5",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.1",
    "redux-persist": "^6.0.0",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.13.7",
    "@babel/preset-env": "^7.13.5",
    "@babel/preset-react": "^7.12.13",
    "@commitlint/cli": "^12.0.0",
    "@svgr/cli": "^5.5.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-minify-dead-code-elimination": "^0.5.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-styled-components": "^1.12.0",
    "copy-webpack-plugin": "^7.0.0",
    "dotenv-webpack": "^6.0.2",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-watch": "^7.0.0",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.0",
    "html-minifier-terser": "^5.1.1",
    "html-webpack-plugin": "^5.2.0",
    "husky": "^7.0.0",
    "jsonminify": "^0.4.1",
    "lint-staged": "^10.5.4",
    "prettier": "^2.3.2",
    "redux-devtools-extension": "^2.13.9",
    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^5.1.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.24.1",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}
